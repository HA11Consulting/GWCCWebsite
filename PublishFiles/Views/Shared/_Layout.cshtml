                                    <a href="mailto:Lisa.Hall277@schools.sa.edu.au" target="_top">Admin email</a>
                                    @Html.ActionLink("Link to Map", "ContactUs", "ContactUs")
                                    <a href="https://www.facebook.com/Grey-Ward-Childrens-Centre-293211371530183/" target="_blank">Facebook</a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-4">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
