                                    @Html.ActionLink("Forms", "ParentsInfo", "Parents")
                                    @Html.ActionLink("Useful Links", "UsefulLinks", "Parents")
                                    @Html.ActionLink("Testimonials", "Testimonials", "Parents")
                                </div>
                            </li>
                            <li class="dropdown">
                                @Html.ActionLink("Rooms", "Babies", "Rooms")
                                <div class="dropdown-content">
                                    @Html.ActionLink("Babies", "Babies", "Rooms")
                                    @Html.ActionLink("Nursery", "Nursery", "Rooms")
                                    @Html.ActionLink("Junior Kindy", "JuniorKindy", "Rooms")
                                    @Html.ActionLink("Kindergarten", "Kindergarten", "Rooms")
