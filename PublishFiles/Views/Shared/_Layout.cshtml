                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-8">
                    <div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav">
                            <li class="dropdown">
                                @Html.ActionLink("Home", "Index", "Home")
                                <div class="dropdown-content">
                                    @Html.ActionLink("Our Philosophy and Goals", "About", "Home")
                                    @Html.ActionLink("Our History", "History", "Home")
                                    @Html.ActionLink("Our Team", "Team", "Home")
                                    @Html.ActionLink("Governance", "Governance", "Home")
                                </div>
                            </li>
                            <li class="dropdown">
                                @Html.ActionLink("Parents and Caregivers", "ParentsInfo", "Parents")
